<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.antailbaxt3r.docblock_doctorapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.antailbaxt3r.docblock_doctorapp.ProfileActivity"></activity>
        <activity android:name="com.antailbaxt3r.docblock_doctorapp.OpenImage" />
        <activity
            android:name="com.antailbaxt3r.docblock_doctorapp.HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.antailbaxt3r.docblock_doctorapp.RegisterActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="com.antailbaxt3r.docblock_doctorapp.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="com.antailbaxt3r.docblock_doctorapp.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>